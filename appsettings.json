{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "EnableSwagger": true,
    "HttpsForce": false,
    "AgentName": "LocalDEV",
    "AuthToken": "",
    "NodeURL": "",
    "PubSub": {
      "Enabled": true,
      "IntervalSeconds": "3",
      "Requests": [
        {
          "Topic": "cqs_emr-send",
          "ExecuteUrl": "cqs_emr-list",
          "ExecuteMethod": 0,
          "CallbackUrl": "",
          "CallbackMethod": 3
        }
      ]
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [    
    "http://localhost"    
  ],
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/logerror-.txt",
          "restrictedToMinimumLevel": "Error",
          "rollingInterval": "Day",
          "flushToDiskInterval": "00:00:03",
          "fileSizeLimitBytes": 10000000,
          "shared": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/logdebug-.txt",
          "restrictedToMinimumLevel": "Debug",
          "rollingInterval": "Day",
          "flushToDiskInterval": "00:00:03",
          "fileSizeLimitBytes": 10000000,
          "shared": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/loginfo-.txt",
          "restrictedToMinimumLevel": "Information",
          "rollingInterval": "Day",
          "flushToDiskInterval": "00:00:03",
          "fileSizeLimitBytes": 10000000,
          "shared": true
        }
      }
    ]
  }
}
